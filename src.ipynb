   "source": [
    "def one_hard_encode(X, K):\n",
    "    N, D = X.shape\n",
    "    Y = np.zeros((N, D, K))\n",
    "    for n, d in zip(*X.nonzero()):\n",
    "        Y[n, d, int(X[n, d]*2 - 1)] = 1\n",
    "    return Y\n",
